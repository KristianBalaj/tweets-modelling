build-type:          Simple
extra-source-files:  CHANGELOG.md

executable db-proj
  main-is:             Main.hs
  hs-source-dirs: src
  ghc-options: -Wall -O3
  other-modules:
    Models.Tweet,
    Models.User,
    Models.Coordinates,
    Models.Country,
    Database.Database,
    Database.TweetsHandling,
    Database.UsersHandling,
    Database.CountriesHandling
  -- other-extensions:
  build-depends:       
    base >=4.13 && <4.14,
    postgresql-simple,
    bytestring,
    aeson,
    zlib,
    text,
    directory,
    filepath,
    containers,
    utf8-string,
    split,
    time
  -- hs-source-dirs:
  default-language:    Haskell2010
